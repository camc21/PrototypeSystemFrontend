rom 'primereact/button';\r\nimport { DataTable } from 'primereact/datatable';\r\nimport { Column } from 'primereact/column';\r\nimport { Fieldset } from 'primereact/fieldset';\r\nimport Select from 'react-select'\r\n\r\nimport { UsuarioDataService } from '../../services/UsuarioDataService';\r\nimport withAuth from '../../components/withAuth';\r\nimport { loadUsuarioSelectedAction, loadUsuarioListAction } from '../../store/actions/usuario';\r\n\r\n\r\n\r\nfunction Usuario(props) {\r\n\r\n    const [usuarioDataPage, setUsuarioDataPage] = useState(useSelector((state) => state.usuarioListReducer.usuarioDataPage));\r\n\r\n    const [usuarioSelected, setUsuarioSelected] = useState(useSelector((state) => state.usuarioSelectedReducer.usuarioSelected));\r\n\r\n    const [itensPerPage, setItensPage] = useState([\r\n        { value: 10, label: 10 },\r\n        { value: 20, label: 20 },\r\n        { value: 50, label: 50 }\r\n    ]);\r\n\r\n    const [rows, setRows] = useState(itensPerPage[0]);\r\n    const [sortBy, setSortBy] = useState(\"nome\");\r\n    const [numberPage, setNumberPage] = useState(0);\r\n\r\n    const toast = useRef(null);\r\n\r\n    const dispatch = useDispatch();\r\n\r\n\r\n    useEffect(() => {\r\n        page(numberPage, rows.value, sortBy);\r\n    }, [rows, numberPage])\r\n\r\n    function page(pageNo, pageSize, sortBy) {\r\n        UsuarioDataService._page(pageNo, pageSize, sortBy).then(response => {\r\n            const responseAux = {\r\n                content: response.data.content,\r\n                empty: response.data.empty,\r\n                first: response.data.first,\r\n                last: response.data.last,\r\n                number: response.data.number,\r\n                numberOfElements: response.data.numberOfElements,\r\n                size: response.data.size,\r\n                totalElements: response.data.totalElements,\r\n                totalPages: response.data.totalPages\r\n            }\r\n            dispatch(loadUsuarioListAction(responseAux));\r\n            setUsuarioDataPage(responseAux);\r\n        }).catch(error => {\r\n            console.log(error.response.status);\r\n            switch (error.response.status) {\r\n                case 404:\r\n                    toast.current.show({ severity: 'warn', summary: 'Aviso', detail: \"Serviço indisponível\", life: 3000 });\r\n                    break;\r\n                \r\n                default:\r\n                    toast.current.show({ severity: 'warn', summary: 'Aviso', detail: error.response.data.message, life: 3000 });\r\n                    break;\r\n            }\r\n            \r\n        })\r\n    }\r\n\r\n    const novo = () => {\r\n        dispatch(loadUsuarioSelectedAction(null));\r\n    }\r\n\r\n    const editar = (rowData) => {\r\n        dispatch(loadUsuarioSelectedAction(rowData));\r\n        setUsuarioSelected(rowData)\r\n    }\r\n\r\n    const _delete = (rowData) => {\r\n        const index = usuarioDataPage.content.indexOf(rowData);\r\n        UsuarioDataService._delete(rowData.id).then(response => {\r\n            toast.current.show({ severity: 'error', summary: 'Erro', detail: 'Usuário ' + rowData.nome + ' excluído com sucesso!', life: 3000 });\r\n            page(numberPage, rows.value, sortBy);\r\n        }).catch(error => {\r\n            toast.current.show({ severity: 'warn', summary: 'Aviso', detail: error.response.data.message, life: 3000 });\r\n        })\r\n    }\r\n\r\n    const actionBodyTemplate = (rowData) => {\r\n        return (\r\n            <>\r\n                <Link href=\"/usuario/form\">\r\n                    <Button style={{ marginRight: '10px' }} icon=\"pi pi-pencil\" className=\"p-button-rounded p-button-success p-mr-2\" onClick={() => editar(rowData)} disabled={!usuarioSelected || !usuarioSelected.id} />\r\n                </Link>\r\n\r\n                <Button icon=\"pi pi-trash\" className=\"p-button-rounded p-button-warning\" onClick={() => _delete(rowData)} disabled={!usuarioSelected || !usuarioSelected.id} />\r\n            </>\r\n        );\r\n    }\r\n\r\n    function selectUsuario(e) {\r\n        if (e.value) {\r\n            dispatch(loadUsuarioSelectedAction(e.value))\r\n            setUsuarioSelected(e.value)\r\n        } else {\r\n            dispatch(loadUsuarioSelectedAction(null))\r\n            setUsuarioSelected(null)\r\n        }\r\n\r\n    }\r\n\r\n    function handleItemPerPage(item) {\r\n        const index = itensPerPage.indexOf(item)\r\n        if (index >= 0) {\r\n            setRows(itensPerPage[index]);\r\n        }\r\n    }\r\n\r\n    function numberPageSum(numberPage) {\r\n        setNumberPage(numberPage--);\r\n    }\r\n\r\n    function numberPageMinus(numberPage) {\r\n        setNumberPage(numberPage++);\r\n    }\r\n\r\n\r\n    const template = {\r\n        'RowsPerPageDropdown': () => {\r\n            return (\r\n                <React.Fragment>\r\n                    <span className=\"mx-1\" style={{ color: 'var(--text-color)', userSelect: 'none' }}>Itens por página: </span>\r\n                    <Select options={itensPerPage} value={rows} onChange={(e) => handleItemPerPage(e)} isDisabled={!usuarioDataPage} />\r\n                </React.Fragment>\r\n            );\r\n        },\r\n        'CurrentPageReport': () => {\r\n            return (\r\n                <span style={{ color: 'var(--text-color)', userSelect: 'none', width: '120px', textAlign: 'center' }}>\r\n                    {usuarioDataPage && usuarioDataPage.number + 1} - {usuarioDataPage && usuarioDataPage.numberOfElements} de {usuarioDataPage && usuarioDataPage.totalElements}\r\n                </span>\r\n            )\r\n        },\r\n        'PrevPageLink': () => {\r\n            return (\r\n                <Button label=\"Anterior\" onClick={() => numberPageMinus(numberPage)} disabled={!usuarioDataPage || usuarioDataPage.first} style={{ marginRight: '10px' }} />\r\n            )\r\n        },\r\n        'NextPageLink': () => {\r\n            return (\r\n                <Button label=\"Próximo\" onClick={() => numberPageSum(numberPage)} disabled={!usuarioDataPage || usuarioDataPage.last} />\r\n            )\r\n        },\r\n    };\r\n\r\n    return (\r\n        <>\r\n            <Toast ref={toast} />\r\n            <Fieldset legend=\"Usuário\">\r\n\r\n                <Link href=\"/usuario/form\">\r\n                    <Button id='new-button' label=\"Novo\" onClick={() => novo({})} />\r\n                </Link>\r\n\r\n                <DataTable\r\n                    scrollHeight=\"400px\"\r\n                    scrollable\r\n                    header=\"Usuários Cadastrados\"\r\n                    value={usuarioDataPage && usuarioDataPage.content}\r\n                    selection={usuarioSelected}\r\n                    onSelectionChange={e => selectUsuario(e)}\r\n                    dataKey=\"id\"\r\n                    paginator\r\n                    paginatorTemplate={template}\r\n                    first={numberPage}\r\n                    rows={rows.value}\r\n                    paginatorClassName=\"justify-content-end\"\r\n                >\r\n                    <Column header=\"Selecionado\" selectionMode=\"single\" headerStyle={{ width: '3em' }}></Column>\r\n                    <Column field=\"nome\" header=\"Nome\" sortable></Column>\r\n                    <Column field=\"login\" header=\"Login\" sortable></Column>\r\n                    <Column field=\"email\" header=\"E-mail\" sortable></Column>\r\n                    <Column field=\"perfis\" header=\"Perfis de Acesso\" sortable></Column>\r\n                    <Column body={actionBodyTemplate}></Column>\r\n                </DataTable>\r\n\r\n                <Link href=\"/\">\r\n                    <Button id='back-button' label=\"Voltar\" />\r\n                </Link>\r\n\r\n            </Fieldset>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default withAuth(Usuario);"]},"metadata":{},"sourceType":"module"}