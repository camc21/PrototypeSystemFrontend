ityName: \"\", readingPermission: false, writingPermission: false, addedData: []});\r\n\r\n    const toast = useRef(null);\r\n    const router = useRouter();\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        FunctionalityDataService._comboBox().then(response => {\r\n            setFunctionalitiesComboBox(response.data);\r\n            console.log(response.data);\r\n            if (response.data.length > 0) {\r\n                setAccessProfileData({...accessProfileData, functionalityName: response.data[0].value})\r\n            }\r\n        }).catch(error => {\r\n            console.log(error.response.status);\r\n            switch (error.response.status) {\r\n                case 401:\r\n                    console.log('1');\r\n                    console.log(error.response);\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: error.response.data.message, life: 5000 });\r\n                    break;\r\n                case 404:\r\n                    console.log('2');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Serviço indisponível\", life: 5000 });\r\n                    break;\r\n                default:\r\n                    console.log('3');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Erro no sistema, contate o administrador\", life: 5000 });\r\n                    break;\r\n            }\r\n        })\r\n    }, []);\r\n\r\n\r\n\r\n    function _save() {\r\n\r\n        if (!accessProfileSelected.id) {\r\n            AccessProfileDataService._post(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro criado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO POST\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n            })\r\n        } else {\r\n            AccessProfileDataService._put(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro alterado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO PUT\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n\r\n            })\r\n        }\r\n    }\r\n\r\n    function onChangeName(e) {\r\n        setAccessProfileData({...accessProfileData, accessProfileName:  e.target.value})\r\n    }\r\n\r\n    function onChangeDescription(e) {\r\n        setAccessProfileData({...accessProfileData, accessProfileDescription: e.target.value})\r\n    }\r\n\r\n    function handleFunctionalityComboBox(item) {\r\n        setAccessProfileData({...accessProfileData, functionalityName: item.value})\r\n        // const index = functionalitiesComboBox.indexOf(item)\r\n        // if (index >= 0) {\r\n        //     setFunctinalitySelected(functionalitiesComboBox[index]);\r\n        // }\r\n    }\r\n\r\n    function onChangeWritingPermission(e) {\r\n        setAccessProfileData({...accessProfileData, writingPermission: e.checked})\r\n    }\r\n\r\n    function onChangeReadingPermission(e) {\r\n        setAccessProfileData({...accessProfileData, readingPermission: e.checked})\r\n    }\r\n\r\n    const addFunctionality =() => {\r\n        let functionalitiesSelectedsDataTableAux = accessProfileData.addedData;\r\n        for (let i = 0; i < functionalitiesComboBox.length; i++) {\r\n            if (functionalitiesComboBox[i].value === accessProfileData.functionalityName) {\r\n                let functionalityPermission = {name: functionalitiesComboBox[i].label, readingPermission: accessProfileData.readingPermission, writingPermission: accessProfileData.writingPermission}\r\n                functionalitiesSelectedsDataTableAux.push(functionalityPermission);\r\n                setAccessProfileData({...accessProfileData, addedData: functionalitiesSelectedsDataTableAux})\r\n                console.log(functionalitiesSelectedsDataTableAux);\r\n                break;\r\n            }\r\n        } \r\n    }\r\n\r\n    function convertBooleanToText(value){\r\n        if(value){\r\n            return \"Sim\";\r\n        } return \"Não\";\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Toast ref={toast} />\r\n            <Fieldset style={{ marginTop: \"15px\" }} legend=\"Cadastro de Perfis de Acesso\">\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"name\">Nome: </label>\r\n                    <InputText className=\"inputfield w-full\" id=\"name\" value={accessProfileData.accessProfileName} onChange={(e) => onChangeName(e)} />\r\n                </div>\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"desciption\">Descrição:</label>\r\n                    <InputText className=\"inputfield w-full\" id=\"description\" value={accessProfileData.accessProfileDescription} onChange={(e) => onChangeDescription(e)} />\r\n                </div>\r\n\r\n                <div className=\"formgroup-inline flex align-items-stretch flex-wrap\">\r\n                    <div className=\"field\">\r\n                        <label htmlFor=\"functionalities\">Funcionalidades:</label>\r\n                        <Dropdown className=\"inputfield w-full\" options={functionalitiesComboBox} value={accessProfileData.functionalityName} onChange={(e) => handleFunctionalityComboBox(e)} />\r\n                    </div>\r\n\r\n                    <div className=\"field-checkbox flex align-items-center justify-content-center\">\r\n                        <Checkbox inputId=\"writingPermission\" checked={accessProfileData.writingPermission} onChange={e => onChangeWritingPermission(e)} />\r\n                        <label htmlFor=\"writingPermission\">Permissão de escrita<