uth\";\r\n\r\nimport \"primeflex/primeflex.css\";\r\n\r\nimport { AccessProfileDataService } from \"../../services/AccessProfileDataService\";\r\nimport { FunctionalityDataService } from \"../../services/FunctionalityDataService\";\r\n\r\nimport { loadAccessProfileSelectedAction } from \"../../store/actions/accessProfile\";\r\n\r\n\r\n\r\nfunction AccessProfileForm(props) {\r\n\r\n    const [accessProfileSelected, setAccessProfileSelected] = useState(useSelector((state) => state.accessProfileSelectedReducer.accessProfileSelected));\r\n\r\n    const [functionalitiesComboBox, setFunctionalitiesComboBox] = useState([]);\r\n    const [functionalitySelected, setFunctinalitySelected] = useState({});\r\n    const [permissionsComboBox, setPermissionsComboBox] = useState([]);\r\n    const [permissionSelected, setPermissionSelected] = useState(null);\r\n   \r\n    const toast = useRef(null);\r\n    const router = useRouter();\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        FunctionalityDataService._comboBox().then(response => {\r\n            setFunctionalitiesComboBox(response.data);\r\n            if(response.data.length > 0){\r\n                setFunctinalitySelected(response.data[0]);\r\n            }\r\n        }).catch(error => {\r\n            console.log(error.response.status);\r\n            switch (error.response.status) {\r\n                case 401:\r\n                    console.log('1');\r\n                    console.log(error.response);\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: error.response.data.message, life: 5000 });\r\n                    break;\r\n                case 404:\r\n                    console.log('2');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Serviço indisponível\", life: 5000 });\r\n                    break;\r\n                default:\r\n                    console.log('3');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Erro no sistema, contate o administrador\", life: 5000 });\r\n                    break;\r\n            }\r\n        })\r\n    }, []);\r\n\r\n    function _save() {\r\n\r\n        if (!accessProfileSelected.id) {\r\n            AccessProfileDataService._post(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro criado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO POST\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n            })\r\n        } else {\r\n            AccessProfileDataService._put(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro alterado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO PUT\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n\r\n            })\r\n        }\r\n    }\r\n\r\n    function onChangeName(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.name = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function onChangeDescription(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.description = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function handleFunctionalityComboBox(item) {\r\n        const index = functionalitiesComboBox.indexOf(item)\r\n        if (index >= 0) {\r\n            setFunctinalitySelected(functionalitiesComboBox[index]);\r\n        }\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Toast ref={toast} />\r\n            <Fieldset style={{marginTop: \"15px\"}} legend=\"Cadastro de Perfis de Acesso\">\r\n\r\n                <div className=\"p-fluid\">\r\n                    <div className=\"p-field p-grid\">\r\n                        <label htmlFor=\"name\" className=\"p-col-12 p-md-2\">Nome: </label>\r\n                        <div className=\"p-col-12 p-md-10\">\r\n                            <InputText id=\"name\" value={accessProfileSelected && accessProfileSelected.name} onChange={(e) => onChangeName(e)} />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"p-field p-grid\">\r\n                        <label htmlFor=\"desciption\" className=\"p-col-12 p-md-2\">Descrição:</label>\r\n                        <div className=\"p-col-12 p-md-10\">\r\n                            <InputText id=\"description\" value={accessProfileSelected && accessProfileSelected.description} onChange={(e) => onChangeDescription(e)} />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"p-field p-grid\">\r\n                        <label htmlFor=\"functionalities\" className=\"p-col-12 p-md-2\">Funcionalidades:</label>\r\n                        <div className=\"p-col-12 p-md-10\">\r\n                        <Select options={functionalitiesComboBox} value={functionalitySelected} onChange={(e) => handleFunctionalityComboBox(e)} />\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"p-field p-grid\">\r\n                        <label htmlFor=\"permission\" className=\"p-col-12 p-md-2\">Permissões:</label>\r\n                        <div className=\"p-col-12 p-md-10\">\r\n                        <Select options={permissionsComboBox} value={permissionSelected} onChange={(e) => handlePermissionComboBox(e)} />\r\n                        </div>\r\n                    </div>\r\n\r\n                </div>\r\n\r\n\r\n                <Link href=\"/accessProfile\">\r\n                    <Button id=\"back-button\" style={{ marginRight: \"15px\" }} label=\"Voltar\" />\r\n                </Link>\r\n\r\n                <Button label=\"Salvar\" onClick={_save} />\r\n\r\n            </Fieldset>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexport default withAuth(AccessProfileForm);"]},"metadata":{},"sourceType":"module"}