;\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Serviço indisponível\", life: 5000 });\r\n                    break;\r\n                default:\r\n                    console.log('3');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Erro no sistema, contate o administrador\", life: 5000 });\r\n                    break;\r\n            }\r\n        })\r\n    }, []);\r\n\r\n\r\n\r\n    function _save() {\r\n\r\n        if (!accessProfileSelected.id) {\r\n            AccessProfileDataService._post(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro criado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO POST\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n            })\r\n        } else {\r\n            AccessProfileDataService._put(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro alterado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO PUT\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n\r\n            })\r\n        }\r\n    }\r\n\r\n    function onChangeName(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.name = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function onChangeDescription(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.description = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function handleFunctionalityComboBox(item) {\r\n        console.log(item);\r\n        setFunctinalitySelected(item.value)\r\n        // const index = functionalitiesComboBox.indexOf(item)\r\n        // if (index >= 0) {\r\n        //     setFunctinalitySelected(functionalitiesComboBox[index]);\r\n        // }\r\n    }\r\n\r\n    function onChangeWritingPermission(e) {\r\n        let accessProfileSelectedAux = { ...accessProfileSelected };\r\n        accessProfileSelectedAux.writingPermission = e.checked;\r\n        setAccessProfileSelected(accessProfileSelectedAux);\r\n    }\r\n\r\n    function onChangeReadingPermission(e) {\r\n        let accessProfileSelectedAux = { ...accessProfileSelected };\r\n        accessProfileSelectedAux.readingPermission = e.checked;\r\n        setAccessProfileSelected(accessProfileSelectedAux);\r\n    }\r\n\r\n    function addFunctionality(func){\r\n        let functionalitiesSelectedsDataTableAux = functionalitiesSelectedsDataTable;\r\n        for (let i = 0; i < functionalitiesComboBox.length; i++) {\r\n            if(functionalitiesComboBox[i].value === func){\r\n                functionalitiesSelectedsDataTableAux.push(functionalitiesComboBox[i].label)\r\n            }\r\n            \r\n        }\r\n        console.log(func);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Toast ref={toast} />\r\n            <Fieldset style={{ marginTop: \"15px\" }} legend=\"Cadastro de Perfis de Acesso\">\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"name\">Nome: </label>\r\n                    <InputText className=\"inputfield w-full\" id=\"name\" value={accessProfileSelected && accessProfileSelected.name} onChange={(e) => onChangeName(e)} />\r\n                </div>\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"desciption\">Descrição:</label>\r\n                    <InputText className=\"inputfield w-full\" id=\"description\" value={accessProfileSelected && accessProfileSelected.description} onChange={(e) => onChangeDescription(e)} />\r\n                </div>\r\n\r\n                <div className=\"formgroup-inline flex align-items-stretch flex-wrap\">\r\n                    <div className=\"field\">\r\n                        <label htmlFor=\"functionalities\">Funcionalidades:</label>\r\n                        <Dropdown className=\"inputfield w-full\" options={functionalitiesComboBox} value={functionalitySelected} onChange={(e) => handleFunctionalityComboBox(e)} />\r\n                    </div>\r\n\r\n                    <div className=\"field-checkbox flex align-items-center justify-content-center\">\r\n                        <Checkbox inputId=\"writingPermission\" checked={accessProfileSelected && accessProfileSelected.writingPermission} onChange={e => onChangeWritingPermission(e)} />\r\n                        <label htmlFor=\"writingPermission\">Permissão de escrita</label>\r\n                    </div>\r\n\r\n                    <div className=\"field-checkbox flex align-items-center justify-content-center\">\r\n                        <Checkbox inputId=\"readingPermission\" checked={accessProfileSelected && accessProfileSelected.readingPermission} onChange={e => onChangeReadingPermission(e)} />\r\n                        <label htmlFor=\"readingPermission\">Permissão de leitura</label>\r\n                    </div>\r\n\r\n                    <div className=\"field flex align-items-center justify-content-center\">\r\n                        <Button id=\"back-button\" label=\"Adicionar\" onClick={() => addFunctionality(functionalitySelected)} />\r\n                    </div>\r\n                </div>\r\n\r\n                <div style={{ width: \"300px\", zIndex: 2 }}>\r\n                    <DataTable\r\n                        scrollHeight=\"300px\"\r\n                        scrollable\r\n                        value={functionalitiesSelectedsDataTable}\r\n                        responsiveLayout=\"scroll\">\r\n                        <Column field=\"name\" header=\"Funcionalidade\"></Column