ckbox inputId=\"writingPermission\" checked={permissions.writingPermission} onChange={e => onChangeWritingPermission(e)} />\r\n                        <label htmlFor=\"writingPermission\">Permissão de escrita</label>\r\n                    </div>\r\n\r\n                    <div className=\"field flex align-items-center justify-content-center\">\r\n                        <Button id=\"back-button\" label=\"Adicionar\" onClick={addFunctionality} />\r\n                    </div>\r\n                </div>\r\n\r\n                <div>\r\n                    <DataTable\r\n                        scrollHeight=\"300px\"\r\n                        scrollable\r\n                        value={dataTablePermission}\r\n                        responsiveLayout=\"scroll\">\r\n                        <Column header=\"Selecionado\" selectionMode=\"single\" headerStyle={{ width: \"3em\" }}></Column>\r\n                        <Column field=\"name\" header=\"Funcionalidade\"></Column>\r\n                        <Column field={(e) => convertBooleanToText(e.readingPermission)} header=\"Leitura\"></Column>\r\n                        <Column field={(e) => convertBooleanToText(e.writingPermission)} header=\"Escrita\"></Column>\r\n                        <Column body={actionBodyTemplate}></Column>\r\n                    </DataTable>\r\n                </div>\r\n\r\n\r\n\r\n                <Link href=\"/accessProfile\">\r\n                    <Button id=\"back-button\" style={{ marginRight: \"15px\" }} label=\"Voltar\" />\r\n                </Link>\r\n\r\n                <Button label=\"Salvar\" onClick={_save} />\r\n\r\n            </Fieldset>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexport default withAuth(AccessProfileForm);"]},"metadata":{},"sourceType":"module"}