t-redux\";\r\nimport Link from \"next/link\"\r\nimport { useRouter } from \"next/router\"\r\n\r\nimport { Fieldset } from \"primereact/fieldset\";\r\nimport { InputText } from \"primereact/inputtext\";\r\nimport { Button } from \"primereact/button\";\r\nimport { Toast } from \"primereact/toast\";\r\nimport Select from \"react-select\"\r\nimport { Checkbox } from 'primereact/checkbox';\r\n\r\n\r\nimport withAuth from \"../../components/withAuth\";\r\n\r\nimport \"primeflex/primeflex.css\";\r\n\r\nimport { AccessProfileDataService } from \"../../services/AccessProfileDataService\";\r\nimport { FunctionalityDataService } from \"../../services/FunctionalityDataService\";\r\n\r\nimport { loadAccessProfileSelectedAction } from \"../../store/actions/accessProfile\";\r\n\r\n\r\n\r\nfunction AccessProfileForm(props) {\r\n\r\n    const [accessProfileSelected, setAccessProfileSelected] = useState(useSelector((state) => state.accessProfileSelectedReducer.accessProfileSelected));\r\n\r\n    const [functionalitiesComboBox, setFunctionalitiesComboBox] = useState([]);\r\n    const [functionalitySelected, setFunctinalitySelected] = useState({});\r\n\r\n    const toast = useRef(null);\r\n    const router = useRouter();\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n        FunctionalityDataService._comboBox().then(response => {\r\n            setFunctionalitiesComboBox(response.data);\r\n            if (response.data.length > 0) {\r\n                setFunctinalitySelected(response.data[0]);\r\n            }\r\n        }).catch(error => {\r\n            console.log(error.response.status);\r\n            switch (error.response.status) {\r\n                case 401:\r\n                    console.log('1');\r\n                    console.log(error.response);\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: error.response.data.message, life: 5000 });\r\n                    break;\r\n                case 404:\r\n                    console.log('2');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Serviço indisponível\", life: 5000 });\r\n                    break;\r\n                default:\r\n                    console.log('3');\r\n                    toast.current.show({ severity: \"warn\", summary: \"Aviso\", detail: \"Erro no sistema, contate o administrador\", life: 5000 });\r\n                    break;\r\n            }\r\n        })\r\n    }, []);\r\n\r\n\r\n\r\n    function _save() {\r\n\r\n        if (!accessProfileSelected.id) {\r\n            AccessProfileDataService._post(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro criado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO POST\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n            })\r\n        } else {\r\n            AccessProfileDataService._put(accessProfileSelected).then(response => {\r\n                toast.current.show({ severity: \"success\", summary: \"Sucesso\", detail: \"Registro alterado com sucesso!\", life: 5000 });\r\n                setTimeout(() => {\r\n                    console.log(\"EXECUTADO PUT\");\r\n                    router.push(\"/accessProfile\");\r\n                }, 3000);\r\n\r\n            })\r\n        }\r\n    }\r\n\r\n    function onChangeName(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.name = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function onChangeDescription(e) {\r\n        let accessProfileAux = { ...accessProfileSelected };\r\n        accessProfileAux.description = e.target.value;\r\n        setAccessProfileSelected(accessProfileAux);\r\n    }\r\n\r\n    function handleFunctionalityComboBox(item) {\r\n        const index = functionalitiesComboBox.indexOf(item)\r\n        if (index >= 0) {\r\n            setFunctinalitySelected(functionalitiesComboBox[index]);\r\n        }\r\n    }\r\n\r\n    function onChangeWritingPermission(e) {\r\n        let accessProfileSelectedAux = { ...accessProfileSelected };\r\n        accessProfileSelectedAux.writingPermission = e.checked;\r\n        setAccessProfileSelected(accessProfileSelectedAux);\r\n    }\r\n\r\n    function onChangeRedingPermission(e) {\r\n        let accessProfileSelectedAux = { ...accessProfileSelected };\r\n        accessProfileSelectedAux.readingPermission = e.checked;\r\n        setAccessProfileSelected(accessProfileSelectedAux);\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Toast ref={toast} />\r\n            <Fieldset style={{ marginTop: \"15px\" }} legend=\"Cadastro de Perfis de Acesso\">\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"name\" className=\"p-col-12 p-md-2\">Nome: </label>\r\n                    <InputText id=\"name\" value={accessProfileSelected && accessProfileSelected.name} onChange={(e) => onChangeName(e)} />\r\n                </div>\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"desciption\" className=\"p-col-12 p-md-2\">Descrição:</label>\r\n                    <InputText id=\"description\" value={accessProfileSelected && accessProfileSelected.description} onChange={(e) => onChangeDescription(e)} />\r\n                </div>\r\n\r\n                <div className=\"field\">\r\n                    <label htmlFor=\"functionalities\" className=\"p-col-12 p-md-2\">Funcionalidades:</label>\r\n                    <Select options={functionalitiesComboBox} value={functionalitySelected} onChange={(e) => handleFunctionalityComboBox(e)} />\r\n                </div>\r\n\r\n                <div className=\"formgroup-inline\">\r\n                    <div className=\"field-checkbox\">\r\n                        <Checkbox inputId=\"writingPermission\" checked={accessProfileSelected && accessProfileSelected.writingPermission} onChange={e => onChangeWritingPermission(e)} />\r\n                        <label htmlFor=\"writingPermission\">Permissão de escrita</label>\r\n                    </div>\r\n\r\n                    <div className=\"field-checkbox\">\r\n                        <Checkbox inputId=\"readingPermission\" checked={accessProfileSelected && accessProfileSelected.readingPermission} onChange={e => onChangeReadingPermission(e)} />\r\n                        <label htmlFor=\"readingPermission\">Permissão de leitura</label>\r\n                    </div>\r\n                </div>\r\n\r\n\r\n                <Link href=\"/accessProfile\">\r\n                    <Button id=\"back-button\" style={{ marginRight: \"15px\" }} label=\"Voltar\" />\r\n                </Link>\r\n\r\n                <Button label=\"Salvar\" onClick={_save} />\r\n\r\n            </Fieldset>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexport default withAuth(AccessProfileForm);"]},"metadata":{},"sourceType":"module"}